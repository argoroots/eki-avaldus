extends /_templates/main.pug


block content
    .container
        h2.col-sm-8.offset-sm-2.text-center.my-5= self.subTitle
        form.col-sm-8.offset-sm-2()
            each i in self.form
                +input(i)
        .row.my-5
            .col-sm-4.offset-sm-4
                button.btn.btn-success.btn-lg.col()= self.submit
